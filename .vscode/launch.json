{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Firefox against localhost",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/src",
      "reAttach": true
    },
    {
      "name": "Attach to Firefox",
      "type": "firefox",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}/src"
    }
  ]
}
